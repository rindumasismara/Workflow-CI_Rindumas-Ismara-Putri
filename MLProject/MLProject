name: Fraud_Detection_Rindumas

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      n_estimators: {type: int, default: 100}
      max_depth: {type: int, default: 10}
      dataset: {type: str, default: onlinepaymentfraud_preprocessing.csv}
    command: >
      python modelling.py
      {n_estimators}
      {max_depth}
      {dataset}
